config_version=5

[application]

config/name="New-tower-bullet-hell"
run/main_scene="res://scenes/main/main.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
run/max_fps=60
config/icon="res://icon.svg"

[autoload]

EnemyAiManager="*res://scripts/autoloads/enemy_AI_manager.gd"
PlayerInfo="*res://scripts/autoloads/player_info.gd"
CombatManager="*res://scripts/autoloads/combat_manager.gd"
EventBus="*res://scripts/autoloads/event_bus.gd"
ParticlesQueueNode="*res://scenes/autoload_scene/particles_queue.tscn"

[debug]

gdscript/warnings/untyped_declaration=1
gdscript/warnings/unsafe_property_access=1

[display]

window/size/viewport_width=640
window/size/viewport_height=480
window/size/window_width_override=1280
window/size/window_height_override=960
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/stretch/scale_mode="integer"

[file_customization]

folder_colors={
"res://assets/": "pink",
"res://exports/": "yellow",
"res://resources/": "green"
}

